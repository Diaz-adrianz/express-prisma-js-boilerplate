  category            String?              @db.VarChar(255)
  cover               String?              @db.VarChar(255)
  created_at          DateTime             @db.DateTime(0)
  updated_at          DateTime             @db.DateTime(0)
  tbl_book_reviews    tbl_book_reviews[]
  ref_book_categories ref_book_categories? @relation(fields: [book_cat_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "tbl_books_ibfk_1")
  tbl_borrow_books    tbl_borrow_books[]

  @@index([book_cat_id], map: "book_cat_id")
}

model tbl_borrow_books {
  id           Int           @id @default(autoincrement())
  student_id   Int?
  book_id      Int?
  start_date   DateTime?     @db.DateTime(0)
  end_date     DateTime?     @db.DateTime(0)
  created_at   DateTime      @db.DateTime(0)
  updated_at   DateTime      @db.DateTime(0)
  tbl_students tbl_students? @relation(fields: [student_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "tbl_borrow_books_ibfk_1")
  tbl_books    tbl_books?    @relation(fields: [book_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "tbl_borrow_books_ibfk_2")

  @@index([book_id], map: "book_id")
  @@index([student_id], map: "student_id")
}

model tbl_class_timetable {
  id           Int           @id @default(autoincrement())
