generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ref_academic_years {
  id         Int      @id @default(autoincrement())
  start      String?  @db.VarChar(255)
  end        String?  @db.VarChar(255)
  name       String?  @db.VarChar(255)
  status     String?  @db.VarChar(255)
  created_at DateTime @db.DateTime(0)
  updated_at DateTime @db.DateTime(0)
}

model ref_book_categories {
  id        Int         @id @default(autoincrement())
  code      String?     @db.VarChar(255)
  category  String?     @db.VarChar(255)
  tbl_books tbl_books[]
}

model ref_classes {
  id                       Int                        @id @default(autoincrement())
  level                    String                     @db.VarChar(255)
  class_name               String                     @db.VarChar(255)
  book_target              Int?
  waste_target             Float?
  created_at               DateTime                   @db.DateTime(0)
  updated_at               DateTime                   @db.DateTime(0)
  ref_narrative_categories ref_narrative_categories[]
  ref_timetable_temp       ref_timetable_temp[]
  tbl_class_timetable      tbl_class_timetable[]
  tbl_form_teacher         tbl_form_teacher[]
}

model ref_financial_posts {
  id         Int      @id @default(autoincrement())
  name       String?  @db.VarChar(255)
  in_out     String?  @db.VarChar(255)
  desc       String?  @db.VarChar(255)
  created_at DateTime @db.DateTime(0)
  updated_at DateTime @db.DateTime(0)
}

model ref_months {
  id     Int     @id @default(autoincrement())
  name   String? @db.VarChar(255)
  status String? @db.VarChar(255)
}

model ref_narrative_categories {
  id                           Int                            @id @default(autoincrement())
  code                         String?                        @db.VarChar(255)
  class_id                     Int?
  category                     String?                        @db.VarChar(255)
  ref_classes                  ref_classes?                   @relation(fields: [class_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "ref_narrative_categories_ibfk_1")
  ref_narrative_sub_categories ref_narrative_sub_categories[]

  @@index([class_id], map: "class_id")
}

model ref_narrative_desc {
  id                           Int                           @id @default(autoincrement())
  narrative_sub_cat_id         Int?
  desc                         String?                       @db.VarChar(255)
  ref_narrative_sub_categories ref_narrative_sub_categories? @relation(fields: [narrative_sub_cat_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "ref_narrative_desc_ibfk_1")

  @@index([narrative_sub_cat_id], map: "narrative_sub_cat_id")
}

model ref_narrative_sub_categories {
  id                       Int                       @id @default(autoincrement())
  narrative_cat_id         Int?
  code                     String?                   @db.VarChar(255)
  sub_category             String?                   @db.VarChar(255)
  ref_narrative_desc       ref_narrative_desc[]
  ref_narrative_categories ref_narrative_categories? @relation(fields: [narrative_cat_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "ref_narrative_sub_categories_ibfk_1")

  @@index([narrative_cat_id], map: "narrative_cat_id")
}

model ref_payment_category {
  id               Int               @id @default(autoincrement())
  payment_post_id  Int?
  academic_year    String?           @db.VarChar(255)
  payment_type     String?           @db.VarChar(255)
  billing_cycle    String?           @db.VarChar(255)
  level            String?           @db.VarChar(255)
  created_at       DateTime          @db.DateTime(0)
  updated_at       DateTime          @db.DateTime(0)
  ref_payment_post ref_payment_post? @relation(fields: [payment_post_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "ref_payment_category_ibfk_1")

  @@index([payment_post_id], map: "payment_post_id")
}

model ref_payment_post {
  id                   Int                    @id @default(autoincrement())
  name                 String?                @db.VarChar(255)
  desc                 String?                @db.VarChar(255)
  ref_payment_category ref_payment_category[]
}

model ref_personality {
  id   Int     @id @default(autoincrement())
  desc String? @db.VarChar(255)
}

model ref_religions {
  id         Int      @id @default(autoincrement())
  code       String?  @db.VarChar(255)
  name       String?  @db.VarChar(255)
  created_at DateTime @db.DateTime(0)
  updated_at DateTime @db.DateTime(0)
}

model ref_roles {
  id           Int     @id @default(autoincrement())
  code         String? @db.VarChar(255)
  name         String  @db.VarChar(255)
  access_right String? @db.VarChar(255)
}

model ref_subjects {
  id                  Int                   @id @default(autoincrement())
  level               String?               @db.VarChar(255)
  code                String?               @db.VarChar(255)
  name                String?               @db.VarChar(255)
  threshold           Float?
  ref_timetable_temp  ref_timetable_temp[]
  tbl_class_timetable tbl_class_timetable[]
}

model ref_task_categories {
  id   Int     @id @default(autoincrement())
  desc String? @db.VarChar(255)
}

model ref_templates {
  id         Int      @id @default(autoincrement())
  code       String?  @db.VarChar(255)
  file_path  String?  @db.VarChar(255)
  desc       String?  @db.VarChar(255)
  created_at DateTime @db.DateTime(0)
  updated_at DateTime @db.DateTime(0)
}

model ref_timetable_temp {
  id           Int           @id @default(autoincrement())
  class_id     Int?
  day_id       Int?
  subject_id   Int?
  semester     Int?
  time_seq     Int?
  created_at   DateTime      @db.DateTime(0)
  updated_at   DateTime      @db.DateTime(0)
  ref_classes  ref_classes?  @relation(fields: [class_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "ref_timetable_temp_ibfk_1")
  ref_weekday  ref_weekday?  @relation(fields: [day_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "ref_timetable_temp_ibfk_2")
  ref_subjects ref_subjects? @relation(fields: [subject_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "ref_timetable_temp_ibfk_3")

  @@index([class_id], map: "class_id")
  @@index([day_id], map: "day_id")
  @@index([subject_id], map: "subject_id")
}

model ref_users {
  id              Int       @default(autoincrement())
  uuid            String    @db.Char(36)
  role_id         Int
  full_name       String?   @db.VarChar(255)
  email           String?   @db.VarChar(255)
  password        String?   @db.VarChar(255)
  status          Int?
  email_verified  Int?
  address         String?   @db.VarChar(255)
  phone_number    String?   @db.VarChar(255)
  avatar          String?   @db.VarChar(255)
  reset_token     String?   @db.VarChar(255)
  reset_token_exp DateTime? @db.DateTime(0)
  created_at      DateTime  @db.DateTime(0)
  updated_at      DateTime  @db.DateTime(0)

  @@id([id, uuid])
}

model ref_wastetypes {
  id   Int     @id @default(autoincrement())
  code String? @db.VarChar(255)
  name String? @db.VarChar(255)
}

model ref_weekday {
  id                  Int                   @id @default(autoincrement())
  name                String?               @db.VarChar(255)
  ref_timetable_temp  ref_timetable_temp[]
  tbl_class_timetable tbl_class_timetable[]
}

model sequelizemeta {
  name String @id @unique(map: "name") @db.VarChar(255)
}

model tbl_achievements {
  id               Int           @id @default(autoincrement())
  student_id       Int?
  achievement_desc String?       @db.VarChar(255)
  issued_at        DateTime?     @db.DateTime(0)
  certificate_path String?       @db.VarChar(255)
  created_at       DateTime      @db.DateTime(0)
  updated_at       DateTime      @db.DateTime(0)
  tbl_students     tbl_students? @relation(fields: [student_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "tbl_achievements_ibfk_1")

  @@index([student_id], map: "student_id")
}

model tbl_announcements {
  id                Int       @id @default(autoincrement())
  date_start        DateTime? @db.DateTime(0)
  date_end          DateTime? @db.DateTime(0)
  announcement_desc String?   @db.Text
  created_at        DateTime  @db.DateTime(0)
  updated_at        DateTime  @db.DateTime(0)
}

model tbl_book_reviews {
  id           Int           @id @default(autoincrement())
  book_id      Int?
  student_id   Int?
  rating       Float?
  comment      String?       @db.Text
  created_at   DateTime      @db.DateTime(0)
  updated_at   DateTime      @db.DateTime(0)
  tbl_books    tbl_books?    @relation(fields: [book_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "tbl_book_reviews_ibfk_1")
  tbl_students tbl_students? @relation(fields: [student_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "tbl_book_reviews_ibfk_2")

  @@index([book_id], map: "book_id")
  @@index([student_id], map: "student_id")
}

model tbl_book_sliders {
  id         Int      @id @default(autoincrement())
  path       String?  @db.VarChar(255)
  title      String?  @db.VarChar(255)
  writer     String?  @db.VarChar(255)
  publisher  String?  @db.VarChar(255)
  created_at DateTime @db.DateTime(0)
  updated_at DateTime @db.DateTime(0)
}

model tbl_books {
  id                  Int                  @id @default(autoincrement())
  book_cat_id         Int?
  title               String?              @db.VarChar(255)
  publisher           String?              @db.VarChar(255)
  writer              String?              @db.VarChar(255)
